{
  "compilerOptions": {

    /* Language and Environment */
    "target": "ES6",                                  

    /* Modules */
    "module": "commonjs",                     
    "rootDir": "./src",                                  
    "esModuleInterop": true,                       
    "forceConsistentCasingInFileNames": true,   
    "strict": true, 
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./src/types"]             
  },
  
  "include": [
    "src/**/*",
    "src/types/express/index.d.ts",
    "src/types/express.d.ts"
  ],
  
    // "files":["types/express/index.d.ts"],

  


}
